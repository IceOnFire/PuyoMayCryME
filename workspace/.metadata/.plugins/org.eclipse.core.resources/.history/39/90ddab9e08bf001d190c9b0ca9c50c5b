package it.ice.game;

public class EntityState extends State {
	public void activate() {
		if (form != null) {
			form.activate();
		}
	}

	public void update() {
		if (form != null) {
			form.update();
		}
	}

	public void dispose() {
		if (form != null) {
			form.dispose();
		}
	}

	public void onEvent(Event event) {
		// TODO Auto-generated method stub
		
	}
}
