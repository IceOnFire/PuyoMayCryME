package it.ice.puyomaycry.actions;

import it.ice.game.Action;
import it.ice.game.Entity;
import it.ice.puyomaycry.states.States;

public class VaIndietroAction extends Action {
	public VaIndietroAction(Entity entity) {
		super(entity);
	}

	protected void run() {
		agent.setActiveState(States.INDIETRO);
	}
}
