package it.ice.puyomaycry.states;

import it.ice.game.Entity;
import it.ice.game.State;
import it.ice.puyomaycry.actions.Actions;

public class CoccolaState extends State {
	public CoccolaState(Entity entity) {
		super(entity);
		// TODO Auto-generated constructor stub
	}

	public void activate() {
		enableAction(Actions.COCCOLA);
		super.activate();
	}
}
